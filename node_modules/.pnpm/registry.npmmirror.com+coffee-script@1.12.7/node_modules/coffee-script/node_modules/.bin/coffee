#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/TL/Dev/代码收藏/打字效果简历/node_modules/.pnpm/registry.npmmirror.com+coffee-script@1.12.7/node_modules/coffee-script/bin/node_modules:/mnt/e/TL/Dev/代码收藏/打字效果简历/node_modules/.pnpm/registry.npmmirror.com+coffee-script@1.12.7/node_modules/coffee-script/node_modules:/mnt/e/TL/Dev/代码收藏/打字效果简历/node_modules/.pnpm/registry.npmmirror.com+coffee-script@1.12.7/node_modules:/mnt/e/TL/Dev/代码收藏/打字效果简历/node_modules/.pnpm/node_modules:/mnt/e/TL/Dev/代码收藏/打字效果简历/node_modules:/mnt/e/TL/Dev/代码收藏/node_modules:/mnt/e/TL/Dev/node_modules:/mnt/e/TL/node_modules:/mnt/e/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/TL/Dev/代码收藏/打字效果简历/node_modules/.pnpm/registry.npmmirror.com+coffee-script@1.12.7/node_modules/coffee-script/bin/node_modules:/mnt/e/TL/Dev/代码收藏/打字效果简历/node_modules/.pnpm/registry.npmmirror.com+coffee-script@1.12.7/node_modules/coffee-script/node_modules:/mnt/e/TL/Dev/代码收藏/打字效果简历/node_modules/.pnpm/registry.npmmirror.com+coffee-script@1.12.7/node_modules:/mnt/e/TL/Dev/代码收藏/打字效果简历/node_modules/.pnpm/node_modules:/mnt/e/TL/Dev/代码收藏/打字效果简历/node_modules:/mnt/e/TL/Dev/代码收藏/node_modules:/mnt/e/TL/Dev/node_modules:/mnt/e/TL/node_modules:/mnt/e/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/coffee" "$@"
else
  exec node  "$basedir/../../bin/coffee" "$@"
fi
